class Solution {
    public int[] solution(int n) {
        int[] answer;
        if(n % 2 == 1){
            answer = new int[n/2+1];
        }else{
            answer = new int[n/2];
        }
        
        int t = 0;
        for(int i = 1; i <= n; i+=2){
            answer[t] = i;
            t++;
        }
        return answer;
    }
}
