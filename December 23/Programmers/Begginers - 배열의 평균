class Solution {
    public double solution(int[] numbers) {
        int sum = 0;
        
        for(int i = 0; i < numbers.length; i++){
            sum += numbers[i];
        }
        
        System.out.println("tt111 : " + sum);
        System.out.println("tt2222 : " + numbers.length);
        double answer = sum;
        answer = answer/ numbers.length;
        
        return answer;
    }
}
